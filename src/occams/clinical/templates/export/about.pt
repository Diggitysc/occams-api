<html i18n:domain="occams.clinical" metal:use-macro="main_template">
  <metal:content-slot fill-slot="content-slot">

  <div class="jumbotron">
    <h1 i18n:translate="">Data Export Tool</h1>
    <p i18n:translate="">
      This service was designed so you can retrieve latest copy
      of non-private records.
    </p>
    <p>
      <span class="glyphicon glyphicon-ok"></span>
      <strong i18n:translate="">
        All private data will be de-identified.
      </strong>
      <a href="${request.route_path('export_faq', _anchor='private-data')}"
        ><span class="glyphicon glyphicon-info-sign"></span></a>

    </p>
    <p>
      <span class="glyphicon glyphicon-ok"></span>
      <span tal:omit-tag="" i18n:translate="">
        The most up-to-date records will be used.
      </span>
      <a href="${request.route_path('export_faq', _anchor='record-version')}"
        ><span class="glyphicon glyphicon-info-sign"></span></a>

    </p>
    <p>
      <span class="glyphicon glyphicon-ok"></span>
      <span tal:omit-tag="" i18n:translate="">
        Select export-styles that suit your needs.
      </span>
      <a href="${request.route_path('export_faq', _anchor='export-style')}"
        ><span class="glyphicon glyphicon-info-sign"></span></a>
    </p>
    <p>
      <a class="btn btn-primary btn-lg"
          role="button"
          href="${request.route_path('export_add')}"
          i18n:translate="">Create an export</a>
    </p>
  </div>

  </metal:content-slot>
</html>
