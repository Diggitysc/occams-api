<div tal:define="oid oid|field.oid;" tal:omit-tag="">
  ${field.start_sequence()}
    <tal:choices tal:repeat="choice values">
      <tal:choice tal:define="(value, title) choice">
        <label for="${oid}-${repeat.choice.index}">
          <input
              tal:attributes="
                class css_class|field.widget.css_class;
                checked value in cstruct"
              type="checkbox"
              name="checkbox"
              value="${value}"
              id="${oid}-${repeat.choice.index}"/>
          ${title}
        </label>
      </tal:choice>
    </tal:choices>
  ${field.end_sequence()}
</div>

