<ul class="details list-inline">
  <li>
    <span class="text-muted" i18n:translate="">Code:</span>
    <span data-bind="text: code"></span>
  </li>
  <li>
    <span class="text-muted" i18n:translate="">Label:</span>
    <span data-bind="text: short_title"></span>
  </li>
  <li>
    <span class="text-muted" i18n:translate="">Consent Date:</span>
    <code data-bind="text: consent_date"></code>
  </li>
  <!-- ko if: start_date -->
    <li>
      <span class="text-muted" i18n:translate="">Lifecycle:</span>
      <code><!-- ko text: start_date --><!-- /ko --> - <!-- ko text: end_date --><!-- /ko --></code>
    </li>
  <!-- /ko -->
  <!-- ko if: termination_form || randomization_form -->
  <li>
    <span class="text-muted" i18n:translate="">Enrollment Forms:</span>
    <!-- ko with: termination_form -->
      <strong data-bind="text: title"></strong>
    <!-- /ko -->
    <!-- ko if: randomization_form -->
      <!-- ko if: termination_form -->
        <span>,</span>
      <!-- /ko -->
      <!-- ko with: randomization_form -->
        <strong data-bind="text: title"></strong>
      <!-- /ko -->
    <!-- /ko -->
  </li>
  <!-- /ko -->
</ul>

